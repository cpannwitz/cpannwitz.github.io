{
  "name": "pnwtz-blog",
  "description": "GatsbyJS blog for pnwtz.me",
  "version": "0.0.11",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "blog",
    "pnwtz"
  ],
  "author": {
    "name": "Christian Pannwitz",
    "email": "ch.von.pa@googlemail.com",
    "url": "https://github.com/cpannwitz"
  },
  "browserslist": [
    ">0.5%",
    "not ie <= 8",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "yarn format",
      "git add ."
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md,mdx}\"",
    "serve": "gatsby serve",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.4.0",
    "@mdx-js/react": "^1.4.0",
    "gatsby": "^2.14.1",
    "gatsby-image": "^2.2.14",
    "gatsby-plugin-catch-links": "^2.1.5",
    "gatsby-plugin-dark-mode": "^1.1.0",
    "gatsby-plugin-feed": "^2.3.7",
    "gatsby-plugin-manifest": "^2.2.6",
    "gatsby-plugin-mdx": "^1.0.33",
    "gatsby-plugin-offline": "^2.2.7",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.2.13",
    "gatsby-plugin-sitemap": "^2.2.12",
    "gatsby-plugin-typography": "^2.3.3",
    "gatsby-remark-images": "^3.1.13",
    "gatsby-remark-prismjs": "^3.3.9",
    "gatsby-source-filesystem": "^2.1.11",
    "gatsby-transformer-remark": "^2.6.15",
    "gatsby-transformer-sharp": "^2.2.7",
    "gitalk": "^1.5.0",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typeface-assistant": "^0.0.75",
    "typography": "^0.16.19",
    "typography-theme-alton": "^0.16.19"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2"
  }
}
