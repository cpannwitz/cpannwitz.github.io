{
  "name": "pnwtz-blog",
  "description": "GatsbyJS blog for pnwtz.me",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "react",
    "blog",
    "pnwtz"
  ],
  "author": {
    "name": "Christian Pannwitz",
    "email": "ch.von.pa@gmail.com",
    "url": "https://github.com/cpannwitz"
  },
  "browserslist": [
    ">0.5%",
    "not ie <= 8",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "yarn format",
      "git add ."
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,json,md,mdx}\"",
    "serve": "gatsby serve",
    "lint": "eslint 'src/**/*.{js,jsx,md,mdx,ts,tsx}'",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "babel-plugin-styled-components": "~1.12.0",
    "gatsby": "^2.32.4",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-catch-links": "^2.10.0",
    "gatsby-plugin-feed": "^2.13.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-react-helmet-canonical-urls": "~1.4.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-simple-analytics": "~1.0.3",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "~3.10.0",
    "gatsby-plugin-typescript": "~2.12.0",
    "gatsby-remark-autolink-headers": "~2.11.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-remark-responsive-iframe": "~2.11.0",
    "gatsby-remark-video": "~1.2.5",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.12.0",
    "prism-themes": "~1.5.0",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-responsive": "~8.2.0",
    "styled-components": "~5.2.1",
    "typescript": "~4.2.2",
    "water.css": "~2.0.0"
  },
  "devDependencies": {
    "@types/node": "~14.14.31",
    "@types/react": "~17.0.2",
    "@types/react-dom": "~17.0.1",
    "@types/react-helmet": "~6.1.0",
    "@types/react-responsive": "~8.0.2",
    "@types/styled-components": "~5.1.7",
    "@typescript-eslint/eslint-plugin": "~4.15.2",
    "@typescript-eslint/parser": "~4.15.2",
    "eslint": "~7.20.0",
    "eslint-config-prettier": "~8.0.0",
    "eslint-plugin-prettier": "~3.3.1",
    "eslint-plugin-react": "~7.22.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1"
  }
}
